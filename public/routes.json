{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'",
        "x-frame-options": "sameorigin",
        "permissions-policy": "geolocation=(), microphone=()"
      }
}